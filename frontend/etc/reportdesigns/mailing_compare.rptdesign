            <method name="open"><![CDATA[importPackage(Packages.com.agnitas.reporting.birt.external.dataset);
mailingsSummaryData = mailingCompareDataSet.getData(selectedMailings, params["companyID"]);
currentrow = 0;

max = 0;
for (i = 0; i < mailingsSummaryData.size(); i++) {
	categoryIndex = mailingsSummaryData.get(i).getCategoryindex();
	value = mailingsSummaryData.get(i).getCount();
	if (categoryIndex == CommonKeys.REVENUE_INDEX) {
	    value = value / 100;
	}

	if (categoryIndex <= CommonKeys.REVENUE_INDEX &amp;&amp; value > max) {
		max = value;
	}
}


reportContext.setGlobalVariable("maxValue", parseInt(max));

reportContext.setGlobalVariable("totalSents", {}); // mailing id to sent number
]]></method>
            <method name="fetch"><![CDATA[importPackage(Packages.com.agnitas.messages);
