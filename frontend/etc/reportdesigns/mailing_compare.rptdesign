	return false;
}
currentRowData = mailingsSummaryData.get(currentrow);

row["categoryKey"] = currentRowData.getCategory();
row["category"] = getCategoryNameByKey(currentRowData.getCategory());
row["category_index"] = currentRowData.getCategoryindex();

// do some i18n stuff
//if( currentRowData.getTargetGroupIndex() == 1) {
	if (currentRowData.getCategoryindex() == CommonKeys.DELIVERED_EMAILS_INDEX) {
		totalSents = reportContext.getGlobalVariable("totalSents")
		totalSents[currentRowData.getMailingId()] = parseInt(currentRowData.getCount());
	}
//}

reportContext.setGlobalVariable("totalSents", totalSents);

row["mailingId"] = currentRowData.getMailingId();
row["mailing_name"] = currentRowData.getMailingName();
row["send_date"] = currentRowData.getSendDate();
row["targetgroup_index"] = currentRowData.getTargetGroupIndex();
row["value"] = currentRowData.getCount();
row["rate"] = currentRowData.getRate();
if (row["rate"] == -1) {
    row["rate"] = 0;
}

currentrow++;
return true;
]]></method>
