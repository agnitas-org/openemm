        <example>
            &lt;jsp:include page="/${emm:ckEditorPath(request)}/ckeditor-emm-helper.jsp"&gt;
                &lt;jsp:param name="toolbarType" value="Classic"/&gt;
            &lt;/jsp:include&gt;
